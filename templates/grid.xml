  <document>
	<head>
	  <style>
	  </style>
	</head>
	<stackTemplate>
	  <collectionList>
		  <grid>
		  <section>
				{% for item in menu %}
				{% if msgid is defined %}
				<lockup onselect="notify('/response/{{msgid}}/{{loop.index}}'); load('{{url}}')" menuURL="{{item.menuurl}}">
				{% else %}
				<lockup menuURL="{{item.menuurl}}" onselect="load('/catalog/{{plugin.id | base64encode}}/{{item.url | base64encode}}')">								
				{% endif %}							
					<img src="{{item.icon}}" width="650" height="360"/>
				 	<title>{{item.title}}</title>				 					 	
				</lockup>
				{% endfor %}
			</section>
		  </grid>
	  </collectionList>
	</stackTemplate>
  </document>
