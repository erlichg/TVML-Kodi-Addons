<document>
	<head>
	  <style>
	  </style>
	</head>
	<stackTemplate>		
		<background>
			<img src="/templates/background.png"/>
		</background>
		<!--<banner>
			<buttonLockup onselect="load('/settings')">
				<badge src="/templates/settings.png" width="50" height="50"/>
			</buttonLockup>
		</banner>-->
	  <collectionList>
		  <grid>
		  <section>
				{% for plugin in menu %}
				<lockup menuURL="{{plugin.menuurl}}" onselect="load('/catalog/{{plugin.id | base64encode}}')">
					<img src="/{{plugin.icon}}" width="256" height="256"/>
				 	<title style="color:rgb(241,241,241)">{{plugin.name}}</title>				 					 	
				</lockup>
				{% endfor %}
			</section>
		  </grid>
	  </collectionList>
	</stackTemplate>
  </document>
