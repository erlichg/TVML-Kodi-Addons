<document>
	<head>
	  <style>
	  </style>
	</head>
	<stackTemplate>
	  <collectionList>
		  <grid>
		  <section>
				{% for plugin in menu %}
				<lockup menuURL="{{plugin.menuurl}}" onselect="load('/catalog/{{plugin.id | base64encode}}')">
					<img src="/{{plugin.icon}}" width="650" height="360"/>
				 	<title>{{plugin.name}}</title>				 					 	
				</lockup>
				{% endfor %}
			</section>
		  </grid>
	  </collectionList>
	</stackTemplate>
  </document>
